{
  "name": "image-fade",
  "version": "1.0.0",
  "description": "A tool for creating *neat-o* transitions between images",
  "main": "index.js",
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-jest": "^23.4.2",
    "babel-preset-env": "^1.7.0",
    "eslint": "^6.3.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^23.4.2",
    "jest-cli": "^23.4.2",
    "jsdoc": "^3.6.3"
  },
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage; open ./coverage/lcov-report/index.html",
    "clean": "rm -r ./out && rm -r ./coverage",
    "doc": "rm -r ./out && ./node_modules/.bin/jsdoc js -r && open ./out/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aarich/image-fade.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aarich/image-fade/issues"
  },
  "jest": {
    "transform": {
      "^.+\\.js": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!@agm)"
    ],
    "verbose": true,
    "coveragePathIgnorePatterns": ["<rootDir>/js/test/"],
    "notify": true
  },
  "homepage": "https://github.com/aarich/image-fade#readme",
  "dependencies": {}
}
